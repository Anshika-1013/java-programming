Google Docs

Supermarket.java
Java
Private comments
public class Supermarket {

    private static int totalItems = 0;

    private final String supermarketName;

    public Supermarket(String name) {
        this.supermarketName = name;
    }

    public void addItem(String itemName, int quantity) {
        
        totalItems += quantity;


        System.out.println(quantity + " " + itemName + "(s) added to the inventory of " + supermarketName);
    }


    public static int getTotalItems() {
        return totalItems;
    }

    public static void main(String[] args) {
        Supermarket supermarket1 = new Supermarket("SuperMart");
        Supermarket supermarket2 = new Supermarket("MegaStore");

        supermarket1.addItem("Bread", 100);
        supermarket2.addItem("Milk", 50);

       
        System.out.println("Total items in all supermarkets: " + Supermarket.getTotalItems());
    }
}
